{
  "id": "titleChange",
  "endPoints": [
    {
      "name": "workersInfo",
      "baseUrlType": "wql",
      "url": "<%'data?query=SELECT%20worker%2C%20firstName%2C%20lastName%2C%20businessTitle%0AFROM%20allActiveEmployees%0AWHERE%20firstName%20CONTAINS%20%22'+ search +'%22%20OR%20lastName%20CONTAINS%20%22'+ search +'%22'%>",
      "authType": "sso",
      "deferred": true
    }
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "name": "workerUpdate",
        "baseUrlType": "workday-common",
        "url": "<% 'workers/' + currentRow.childrenMap.colEmployeeId.value + '/businessTitleChanges' %>",
        "httpMethod": "POST",
        "authType": "sso",
        "exclude": "<% !submitBtn.value %>",
        "onSend": "<% self.data %>"
      },
      {
        "type" : "outboundVariable",
        "variableScope" : "flow",
        "name" : "transitionOutboundVars",
        "values" : [

          {
            "outboundPath": "cancelClicked",
            "value": "<% false %>"
          },
          {
            "outboundPath": "continueClicked",
            "value": "<% false %>"
          },
          {
            "outboundPath": "searchClicked",
            "value": "<% searchBtn.value %>"
          },
          {
            "outboundPath": "submitClicked",
            "value": "<% submitBtn.value %>"
          },
          {
            "outboundPath": "selectedRows",
            "value": "<% !empty(dataGrid.getSelectedRows()) %>"
          },
          {
            "outboundPath": "add_filter_Clicked",
            "value": "<%  %>"
          },
          {
            "outboundPath": "titles",
            "value": "<%
                      var titlesList = dataGrid.getRows().map(x => {
                          var businessTitle = x.getChildrenMap().get('colBusinessTitle').value;
                          return businessTitle;
                      });
                      titlesList;
                      %>"
          }
        ]
      }
    ]
  },
  "presentation": {
    "standardEditButtonsHidden": true,
    "pageType": "edit",
    "title": {
      "type": "title",
      "label": "Test Dynamic Grid"
    },
    "cancelOverride": {
      "taskId": "root"
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "sorted": false,
      "children": [
        {
          "type":"editButtonBar",
          "editButtons": [
            {
              "type": "editButton",
              "label": "Submit",
              "id": "submitBtn",
              "buttonType": "PRIMARY",
              "ignoreRequiredFields": "true",
              "visible":"<% flowVariables.search_filter_Clicked ?? false%>"
            },
            {
              "type": "editButton",
              "label": "Search",
              "id": "searchBtn",
              "buttonType": "AUXILIARY",
              "ignoreRequiredFields": "true",
              "visible":false

            }
          ]
        },
        {
          "type": "section",
          "horizontal": true,
          "children": [
            {
              "type": "fieldSet",
              "id": "fieldSetContainer",
              "children": [
                {
                  "type": "button",
                  "id": "addfilterBtn",
                  "label": "Add new filter",
                  "taskReference": {
                    "type": "taskReference",
                    "taskId": "addFilter"
                  },
                  "action": "AUXILIARY"
                },
                {
                  "type": "readOnlyText",
                  "id": "current_filter",
                  "label": "You are now filtering workers by:",
                  "value": "<%
                            (flowVariables.cancelClicked ?? false) ? flowVariables.previousSearch : flowVariables.search;
                            %>"
                },


                {
                  "type": "grid",
                  "id": "dataGrid",
                  "gridType": "LIST",
                  "rows":
                    "<%


                    var searchTxt = (flowVariables.cancelClicked ?? false) ? flowVariables.previousSearch : flowVariables.search;
                    var test = searchTxt ? workersInfo.invoke({'search': searchTxt}).data : [];
                    pageVariables.previousData = list:mapAttribute(test, 'businessTitle');
                    test;

                     %>",
                  "rowVariableName": "row",
                  "indexVariableName": "i",
                  "_doNotAdd": "true",
                  "_doNotRemoveRow": "true",
                  "enabled": true,
                  "selectionEnabled": true,
                  "currentRowVariable": "currentRow",
                  "_isArrayOutBinding": false,
                  "rowSelected": "<%
                                  if (flowVariables.cancelClicked == true)
                                    flowVariables.titles.get(i) != pageVariables.previousData.get(i);
                                  else
                                    false;
                                  %>",
                  "columns": [
                    {
                      "type": "column",
                      "columnId": "colEmployeeId",
                      "label": "Employee ID",
                      "cellTemplate": {
                        "type": "text",
                        "id": "celdaEID",
                        "value": "<% row.worker.id %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colFirstName",
                      "label": "First Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.firstName %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colLastName",
                      "label": "Last Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.lastName %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colBusinessTitle",
                      "label": "Business Title",
                      "cellTemplate": {
                        "type": "text",
                        "id": "celdaBT",
                        "value": "<% (flowVariables.cancelClicked ?? false) ? flowVariables.titles[i] : row.businessTitle %>",
                        "valueOutBinding": "workerUpdate.proposedBusinessTitle"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    "footer": {
      "type": "footer",
      "children": [
        {
          "type": "richText",
          "enabled": "false",
          "visible":"false",
          "value": "Powered By Daniel & Andrei"
        }
      ]
    }
  }
}