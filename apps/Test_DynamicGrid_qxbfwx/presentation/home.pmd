{
  "id": "home",
  "presentation": {
    "pageType": "edit",
    "title": {
      "type": "title",
      "label": "Test Dynamic Grid"
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "sorted": false,
      "children": [
        {
          "type": "section",
          "horizontal": true,
          "children": [
            {
              "type": "fieldSet",
              "id": "fieldSetContainer",
              "children": [
                {
                  "type" : "dropdown",
                  "label" : "Filter by workers",
                  "id": "country",
                  "idKey" : "ISO_3166_1_Alpha_3_Code",
                  "displayKey" : "name",
                  "values" : "<% countries.items %>"
                },
                {
                  "type": "section",
                  "horizontal": true,
                  "children": [
                    {
                      "type": "button",
                      "id": "filter",
                      "label": "FILTER",
                      "taskReference": {
                        "type": "taskReference",
                        "taskId": "root"
                      },
                      "action": "AUXILIARY"
                    }
                  ]
                },
                {
                  "type": "grid",
                  "id": "dataGrid",
                  "gridType": "LIST",
                  "rows": "<% [] %>",
                  "rowVariableName": "row",
                  "doNotAdd": "true",
                  "doNotRemoveRow": "true",
                  "enabled": true,
                  "columns": [
                    {
                      "type": "column",
                      "columnId": "colEmployeeId",
                      "label": "Employee ID",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.column1 %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colFirstName",
                      "label": "First Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.column2 %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colLastName",
                      "label": "Last Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.column3 %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colBusinessTitle",
                      "label": "Business Title",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.column4 %>"
                      }
                    }
                  ]
                }

              ]
            }
          ]
        }
      ]
    },
    "footer": {
      "type": "footer",
      "children": [
        {
          "type": "richText",
          "enabled": "false",
          "value": "Powered By Daniel & Andrei"
        }
      ]
    }
  }
}