{
  "id": "addFilter",
  "endPoints": [
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "type" : "outboundVariable",
        "variableScope" : "flow",
        "name" : "transitionOutboundVars",
        "values" : [
          {
            "outboundPath" : "search_filter_Clicked",
            "value" : "<% continueBtn.value %>"
          },
          {
            "outboundPath" : "search",
            "value" : "<% searchNameTxt.value %>"
          }
        ]
      }
    ]
  },
  "presentation": {
    "micro": true,
    "microFirstPageOnly": true,
    "headerSize": "VPS_DEFAULT",
    "pageType": "edit",
    "title": {
      "type": "title",
      "label": "Add new filter"
    },
    "standardEditButtonsHidden" : true,
    "body": {
      "type": "section",
      "horizontal": false,
      "children": [
        {
          "type": "text",
          "id": "searchNameTxt",
          "label": "Please enter a name to search"
        },
        {
          "type":"editButtonBar",
          "editButtons": [
            {
              "type": "editButton",
              "label": "Search",
              "id": "continueBtn",
              "buttonType": "PRIMARY",
              "ignoreRequiredFields": "true"
            }
          ]
        }
      ]
    }
  }
}

