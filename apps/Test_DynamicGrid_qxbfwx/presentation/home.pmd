{
  "id": "home",
  "endPoints": [
    {
      "name": "workers",
      "baseUrlType": "workday-staffing",
      "url": "workers",
      "authType": "sso"
    },
    {
      "name": "workersInfo",
      "baseUrlType": "wql",
      "url": "<%'data?query=SELECT%20worker%2C%20firstName%2C%20lastName%2C%20businessTitle%0AFROM%20allActiveEmployees%0AWHERE%20firstName%20CONTAINS%20%22'+ search +'%22%20OR%20lastName%20CONTAINS%20%22'+ search +'%22'%>",
      "authType": "sso",
      "deferred": true
    }
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "name": "workerUpdate",
        "baseUrlType": "workday-common",
        "url": "<% 'workers/' + currentRow.childrenMap.colEmployeeId.value + '/businessTitleChanges' %>",
        "httpMethod": "POST",
        "authType": "sso",
        "onSend": "<%
                   currentRow.selected ? self.data : null;
                   %>"
      }
    ]
  },

  "presentation": {
    "pageType": "edit",
    "title": {
      "type": "title",
      "label": "Test Dynamic Grid"
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "sorted": false,
      "children": [
        {
          "type": "section",
          "horizontal": true,
          "children": [
            {
              "type": "fieldSet",
              "id": "fieldSetContainer",
              "children": [
                {
                  "type": "button",
                  "action": "PRIMARY",
                  "label": "Add a new filter",
                  "taskReference": {
                    "taskId": "filter"
                  }
                },
                {
                  "type": "grid",
                  "id": "dataGrid",
                  "gridType": "LIST",
                  "rows": "<% [] %>",
                  "rowVariableName": "row",
                  "_doNotAdd": "true",
                  "_doNotRemoveRow": "true",
                  "enabled": true,
                  "selectionEnabled": true,
                  "currentRowVariable": "currentRow",
                  "_isArrayOutBinding": false,
                  "columns": [
                    {
                      "type": "column",
                      "columnId": "colEmployeeId",
                      "label": "Employee ID",
                      "cellTemplate": {
                        "type": "text",
                        "id": "celdaEID",
                        "value": "<% row.worker.id %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colFirstName",
                      "label": "First Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.firstName %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colLastName",
                      "label": "Last Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.lastName %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colBusinessTitle",
                      "label": "Business Title",
                      "cellTemplate": {
                        "type": "text",
                        "id": "celdaBT",
                        "value": "<% row.businessTitle %>",
                        "valueOutBinding": "workerUpdate.proposedBusinessTitle"
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    "footer": {
      "type": "footer",
      "children": [
        {
          "type": "richText",
          "enabled": "false",
          "value": "Powered By Daniel & Andrei"
        }
      ]
    }
  }
}