{
  "id": "filter",
  "endPoints": [
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "type": "outboundVariable",
        "name": "flowVariables",
        "variableScope": "flow"
      }
    ]
  },
  "presentation": {
    "headerSize": "VPS_DEFAULT",
    "micro": "true",
    "microFirstPageOnly": "true",
    "title": {
      "type": "title",
      "label": "Workday Extend",
      "enabled": false
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "sorted": false,
      "children": [
        {
          "type": "section",
          "horizontal": true,
          "children": [
            {
              "type": "fieldSet",
              "id": "fieldSetContainer",
              "children": [
                {
                  "type": "text",
                  "id": "searchNameTxt",
                  "label": "Please enter a name to search"
                },
                {
                  "type": "button",
                  "id": "searchBtn",
                  "visible": true,
                  "label": "Search",
                  "valueOutBinding": "flowVariables.searchNameTxt",
                  "taskReference": {
                    "taskId": "home",
                    "parameterBindings": {
                      "search": "<% searchNameTxt.value %>"
                    }
                  }
                }

              ]
            }
          ]
        }
      ]
    },


    "footer": {
      "type": "footer",
      "children": [
        {
          "type": "richText",
          "enabled": "false",
          "value": "Powered By Workday Extend"
        }
      ]
    }
  }
}