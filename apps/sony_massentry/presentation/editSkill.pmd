      "authType": "sso"
    },
    {
      "name": "getMySkills",
      "baseUrlType": "WORKDAY",
      "url": "/customObject/v2/workers/<% personalInfo.id %>/customObjects/skills",
      "authType": "sso",
      "isCollection": "true"
    },
    {
      "name": "userLanguage",
      "baseUrlType": "WORKDAY",
      "authType": "sso",
      "url": "/common/v1/userInfo"
    },
    {
      "name": "getCustOrg",
      "baseUrlType": "WORKDAY",
      "url": "/raas/7000017312/WCP_SS_Worker_CustomOrg?Worker%21WID=<% personalInfo.id %>&format=json",
      "authType": "sso"
    },
    {
      "name": "custOrgInfoAux",
      "baseUrlType": "WORKDAY",
      "deferred": true,
      "url": "/customObject/v2/customObjects/ssconfiguration/<% id %>",
      "authType": "sso"
    },
    {
      "name": "custOrgInfo",
      "deferred": true,
      "url": "https://api.workday.com/customObject/v2/customObjects/ssconfiguration/<% id %>",
      "authType": "sso"
    },
    {
      "name": "getCatalog",
      "baseUrlType": "WORKDAY",
      "deferred": true,
      "url": "/raas/7000017312/WCP_SS_Catalog_Report_v2?workdayID=<% id %>&referenceId=<% queryParams.idSkill %>&format=json",
      "authType": "sso"
    },
    {
      "name": "getCategoryLabels",
      "baseUrlType": "WORKDAY",
      "deferred": true,
      "url": "/raas/7000017312/WCP_SS_Category_Labels_Report?workdayID=<% id %>&format=json",
      "authType": "sso"
    },
    {
      "name": "ratingModels",
      "baseUrlType": "WORKDAY",
      "deferred": true,
      "url": "/raas/7000017312/WCP_SS_Rating_Model?workdayID=<% id %>&ratingModelId=<% list:filter(pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'],'refId',queryParams.idSkill)[0]['ratingModelID'] %>&format=json",
      "authType": "sso"
    }
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "name": "editSkillPUT",
        "url": "/customObject/v2/customObjects/skills/<% queryParams.idSkillUser %>",
        "authType": "sso",
        "exclude": "<% empty assessedRating.value[0] %>",
        "baseUrlType": "WORKDAY",
        "httpMethod": "PUT",
        "values": [
          {
            "outboundPath": "worker.id",
            "value": "<% personalInfo.id %>"
          },
          {
            "outboundPath": "rating",
            "value": "<% number:convertStringToInt(assessedRating.value[0]) %>"
          },
          {
            "outboundPath": "status",
            "value": "<% pageVariables.custOrgInfo.requiresApproval ? 'Awaiting approval' : 'Completed' %>"
          },
          {
            "outboundPath": "assessedOn",
            "value": "<% date:getTodaysDate(date:getDateTimeZone(personalInfo.timezone)) %>"
          },
          {
            "outboundPath": "assessedBy",
            "value": "<% null %>"
          },
          {
            "outboundPath": "approvedOn",
            "value": "<% pageVariables.custOrgInfo.requiresApproval ? null : date:getTodaysDate(date:getDateTimeZone(personalInfo.timezone)) %>"
          },
          {
            "outboundPath": "comment",
            "value": "<% !empty(newComment.value) ? newComment.value : null %>"
          }
        ]
      },
      {
        "name": "flowEditSkill",
        "type": "outboundVariable",
        "variableScope": "flow",
        "values": [
          {
            "outboundPath": "idSkill",
            "value": "<% queryParams.idSkill %>"
          },
          {
            "outboundPath": "idSkillUser",
            "value": "<% queryParams.idSkillUser %>"
          }
        ]
      }
    ]
  },
  "script": "<%
  var getTopCustOrgPriority = function(getCustOrgData){

    var topPriority = 0;
    var topCustOrg = '';

    for(var custOrg : getCustOrgData['Report_Entry'][0]['WCP_TOP_CUSTOM_ORGS_group']){

        var auxCustOrg = custOrg.custom_org_top_wid1;
        var auxPriority = custOrgInfoAux.invoke({'id':auxCustOrg}).priority;

        if(auxPriority > topPriority){
            topPriority = auxPriority;
            topCustOrg = auxCustOrg;
        }
    }
    return topCustOrg;
  };
    var evaluateSubmission = function(){
      var exp = false;
      if(!empty(assessedRating.value) && assessedRating.value[0] != currentRating.value){
          exp=true;
      }else if(!empty(newComment.value) && (newComment.value != list:filter(getMySkills.data, 'skillId', queryParams.idSkill)[0].comment)){
                  exp=true;
            }

      return exp;
   };
%>",
  "onLoad": "<%
      pageVariables.custOrgInfo = custOrgInfo.invoke({'id':getTopCustOrgPriority(getCustOrg)});
      pageVariables.getCatalog = getCatalog.invoke({'id':getTopCustOrgPriority(getCustOrg)});
      pageVariables.getCategoryLabels = getCategoryLabels.invoke({'id':getTopCustOrgPriority(getCustOrg)});
      pageVariables.ratingModels = ratingModels.invoke({'id':getTopCustOrgPriority(getCustOrg)});
    %>",
