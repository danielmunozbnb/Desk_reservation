            {
              "cellTemplate": {
                "type": "text",
                "value": "<% queryParams.language == 'ja' ?  (list:filter(getMySkills.data, 'skillId', queryParams.idSkill)[0].status == 'Completed' ? '完了' : '差戻し履歴') : list:filter(getMySkills.data, 'skillId', queryParams.idSkill)[0].status %>"
              },
              "columnId": "v8",
              "label": "<% presentationLabels.status %>",
              "type": "column"
            }
          ]
        },
        {
          "type": "text",
          "id": "currentRating",
          "label": "<% presentationLabels.currentAssessedRating %>",
          "value": "<% list:filter(getMySkills.data, 'skillId', queryParams.idSkill)[0].rating %>",
          "enabled": "false"
        },
        {
          "type": "text",
          "label": "<% presentationLabels.currentAssessedDate %>",
          "value": "<% list:filter(getMySkills.data, 'skillId', queryParams.idSkill)[0].assessedOn %>",
          "enabled": "false"
        },
        {
          "type": "hidden",
          "id": "regExp",
          "value": "'[' + <% pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'][0]['ratingFrom'] %> + '-' + <% pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'][0]['ratingTo']==10 ? 9 : pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'][0]['ratingTo'] %> + ']|' + <% pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'][0]['ratingTo']==10 ? 10 : pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'][0]['ratingFrom'] %>"
        },
        {
          "type": "section",
          "horizontal": "true",
          "children": [
            {
              "type": "instanceList",
              "label": "<% presentationLabels.newAssessedRating %>",
              "id": "assessedRating",
              "required": "true",
              "instanceListLoopTag": {
                "type": "instanceLoop",
                "templateInstance": {
                  "id": "<% ratingElem.rating %>",
                  "descriptor": "<% ratingElem.rating %>. <% ratingElem.ratingDescriptionText %>"
                },
                "on": "<% list:sort(pageVariables.ratingModels['Report_Entry'][0]['Skills_Survey_Rating_Model_group'], 'rating',true) %>",
                "as": "ratingElem"
              }
            }
          ]
        },
        {
          "type": "textArea",
          "id": "newComment",
          "label": "<% presentationLabels.addComment %>",
          "value": "<% list:filter(getMySkills.data, 'skillId', queryParams.idSkill)[0].comment %>",
          "required": "<% pageVariables.getCatalog['Report_Entry'][0]['Skills_Survey_Catalog_group'][0]['mandatoryComment']==1 %>"
        },
        {
          "type": "text",
          "id": "ratingConfirmation",
          "enabled": "false",
          "validations": [
            {
              "expression": "<% evaluateSubmission(); %>",
              "errorMessage": "Must update either rating or comment",
              "onSubmitOnly": true
            }
          ]
        },
        {
          "type": "editButtonBar",
          "editButtons": [
            {
              "type": "editButton",
              "label": "<% presentationLabels.submitEdit %>",
              "id": "submit",
              "buttonType": "SECONDARY"
            }
          ]
        }
      ]
    }
  }
}