{
  "id": "home",
  "endPoints": [
    {
      "name": "workers",
      "baseUrlType": "workday-staffing",
      "url": "workers",
      "authType": "sso"
    },
    {
      "name": "workersInfo",
      "baseUrlType": "wql",
      "url": "<%'data?query=SELECT%20worker%2C%20firstName%2C%20lastName%2CbusinessTitle%0AFROM%20allWorkers%0AWHERE%20worker%3D%22'+ id + '%22'%>",
      "authType": "sso",
      "deferred": true
    }
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "name": "workerUpdate",
        "baseUrlType": "app",
        "url": "/workers/<% 'me' %>/businessTitleChanges",
        "httpMethod": "POST",
        "authType": "sso",
        "_onSend": "<%  %>",
        "_values": [
          {
            "_outboundPath": "image.id",
            "_value": "<% (!empty(createAnEventImage.id)) ? createAnEventImage.id : null %>"
          }
        ]
      }
    ]
  },

  "presentation": {
    "pageType": "edit",
    "title": {
      "type": "title",
      "label": "Test Dynamic Grid"
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "sorted": false,
      "children": [
        {
          "type": "section",
          "horizontal": true,
          "children": [
            {
              "type": "fieldSet",
              "id": "fieldSetContainer",
              "children": [
                {
                  "type": "text",
                  "id": "searchName",
                  "label": "Please enter a name to search",
                  "onChange": "<%
                               // Call the workerData endpoint with the user-entered name as a parameter.
                               var test = workersInfo.invoke({'id': self.value});
                               // Make the grid visible and set its row data.

                               dataGrid.setRows(test.data);
                               %>"
                },
                {
                  "type": "grid",
                  "id": "dataGrid",
                  "gridType": "LIST",
                  "rows": "<% [] %>",
                  "rowVariableName": "row",
                  "_doNotAdd": "true",
                  "_doNotRemoveRow": "true",
                  "enabled": true,
                  "selectionEnabled": true,
                  "columns": [
                    {
                      "type": "column",
                      "columnId": "colEmployeeId",
                      "label": "Employee ID",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.worker.id %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colFirstName",
                      "label": "First Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.firstName %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colLastName",
                      "label": "Last Name",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.lastName %>"
                      }
                    },
                    {
                      "type": "column",
                      "columnId": "colBusinessTitle",
                      "label": "Business Title",
                      "cellTemplate": {
                        "type": "text",
                        "value": "<% row.businessTitle %>"
                      }
                    }
                  ]
                }

              ]
            }
          ]
        }
      ]
    },
    "footer": {
      "type": "footer",
      "children": [
        {
          "type": "richText",
          "enabled": "false",
          "value": "Powered By Daniel & Andrei"
        }
      ]
    }
  }
}